{
  "name": "hassanbazzi",
  "version": "1.0.0",
  "scripts": {
    "dev": "preact watch",
    "start": "superstatic build -p ${PORT:-8080} --host 0.0.0.0 --gzip -c '{\"rewrites\": [{\"source\":\"**\",\"destination\":\"index.html\"}],\"headers\":[{\"source\":\"**\",\"headers\":[{\"key\":\"Cache-Control\",\"value\":\"max-age=86400\"}]}]}'",
    "prestart": "npm run build",
    "build": "preact build",
    "prebuild": "rm -rf build/assets && mkdir -p build"
  },
  "dependencies": {
    "preact": "8.2.7",
    "preact-google-recaptcha": "^0.2.1",
    "preact-render-to-string": "^4.1.0",
    "window-or-global": "1.0.1",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "eslint": "^4.18.2",
    "eslint-plugin-react": "^7.7.0",
    "mocha": "^5.0.4",
    "mocha-standard": "1.0.0",
    "node-sass": "^4.7.2",
    "preact-cli": "3.0.0-rc.1",
    "preact-cli-plugin-netlify": "^1.1.0",
    "sass-loader": "^6.0.7"
  },
  "devDependencies": {
  }
}
